

{
  "manifest_version": 3,
  "name": "自定义Web背景图片",
  "version": "1.1",
  "description": "为你的网页添加个性化背景图片，并保存设置",
  "permissions": [
  "activeTab",
  "scripting",
  "storage",
  "https://api.siliconflow.cn/"
],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}